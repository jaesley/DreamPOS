<% if flash[:notice] %>
<div class="alert alert-info">
  <%= flash[:notice] %>
</div>
<% end %>

<% if flash[:success] %>
<div class="alert alert-success">
  <%= flash[:success] %>
</div>
<% end %>

<% if flash[:error] %>
<div class="alert alert-danger">
  <%= flash[:error] %>
</div>
<% end %>

<% if flash[:alert] %>
<div class="alert alert-danger">
  <%= flash[:alert] %>
</div>
<% end %>
